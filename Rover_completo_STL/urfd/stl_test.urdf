<?xml version="1.0"?>
<robot name="rover">
  <link name="world">
  </link>


<joint name="world_to_base" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>

  <link name="base_link">

    <!-- VISUAL -->
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
	 <mesh filename="file:///app/stl/rover_caixa.stl" scale="0.01 0.01 0.01"/>
      </geometry>
      <material name="gray">
        <color rgba="0.6 0.6 0.6 1.0"/>
      </material>
    </visual>

    <!-- COLLISION -->
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
	 <mesh filename="file:///app/stl/rover_caixa.stl" scale="0.01 0.01 0.01"/>
      </geometry>
    </collision>



  </link>


</robot>

